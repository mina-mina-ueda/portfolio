<div class="container">
  <div class="row">
    <h3>会員一覧</h3>
  </div>
  <table>
    <thead>
      <th>会員ID</th>
      <th>ニックネーム</th>
      <th>年齢</th>
      <th>性別</th>
      <th>職業</th>
      <th>来店手段</th>
    </thead>
    <tbody>
  		<%# customers.each do |customer| %>
  	    <tr>
    		  <td><%#= customer.id %></td>
    		  <td><%#= customer.nickname %></td>
    		  <td><%#= customer.age %></td>
    		  <td><%#= customer.gender %></td>
    		  <td><%#= customer.work %></td>
    		  <td><%#= customer.accsess %></td>
        </tr>
      <%# end %>
    </tbody>
  </table>
</div>